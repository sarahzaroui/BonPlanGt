<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Commentaire" table="Commentaire">
    <indexes>
      <index name="AK_Identifier_1" columns="idCommentaire"/>
      <index name="FK_Client_Commen" columns="idUser"/>
      <index name="FK_Art_Comm" columns="idArticle"/>
      <index name="FK_Even_Commen" columns="idEv"/>
    </indexes>
    <id name="idcommentaire" type="integer" column="idCommentaire">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="description" type="string" column="description" length="254" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idarticle" target-entity="Article" fetch="LAZY">
      <join-columns>
        <join-column name="idArticle" referenced-column-name="idArticle"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="iduser" target-entity="Client" fetch="LAZY">
      <join-columns>
        <join-column name="idUser" referenced-column-name="idUser"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idev" target-entity="Evennement" fetch="LAZY">
      <join-columns>
        <join-column name="idEv" referenced-column-name="idEv"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
