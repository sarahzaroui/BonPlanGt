<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Galerie" table="Galerie">
    <indexes>
      <index name="FK_Even_Galerie" columns="idEv"/>
      <index name="FK_Art_Gal" columns="idArticle"/>
    </indexes>
    <id name="idgalerie" type="integer" column="idGalerie">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="254" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nomgalerie" type="string" column="nomGalerie" length="254" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idarticle" target-entity="Article" fetch="LAZY">
      <join-columns>
        <join-column name="idArticle" referenced-column-name="idArticle"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idev" target-entity="Evennement" fetch="LAZY">
      <join-columns>
        <join-column name="idEv" referenced-column-name="idEv"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
