<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Client" table="Client">
    <indexes>
      <index name="FK_NewsL_Client" columns="idNewsl"/>
    </indexes>
    <id name="iduser" association-key="true"/>
    <field name="nom" type="string" column="nom" length="254" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="254" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="situationfamiliale" type="string" column="situationFamiliale" length="254" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="genre" type="string" column="genre" length="254" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="age" type="integer" column="age" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <one-to-one field="iduser" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="idUser" referenced-column-name="idUser"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="idnewsl" target-entity="Newsletter" fetch="LAZY">
      <join-columns>
        <join-column name="idNewsl" referenced-column-name="idNewsl"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
