<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Note" table="Note">
    <indexes>
      <index name="AK_Identifier_1" columns="idNote"/>
      <index name="FK_Art_Note" columns="idArticle"/>
    </indexes>
    <id name="idnote" type="integer" column="idNote">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="note" type="integer" column="note" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="idarticle" target-entity="Article" fetch="LAZY">
      <join-columns>
        <join-column name="idArticle" referenced-column-name="idArticle"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
