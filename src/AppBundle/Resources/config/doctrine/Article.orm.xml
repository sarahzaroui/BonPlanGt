<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Article" table="Article">
    <indexes>
      <index name="AK_Identifier_1" columns="idArticle"/>
      <index name="FK_Client_Art" columns="idUser"/>
      <index name="FK_Art_Resgion" columns="idRegion"/>
      <index name="FK_Art_CatArt" columns="idCatArt"/>
    </indexes>
    <id name="idarticle" type="integer" column="idArticle">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="254" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="254" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="datetime" column="date" nullable="true"/>
    <field name="etat" type="string" column="etat" length="254" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idcatart" target-entity="Categoriearticle" fetch="LAZY">
      <join-columns>
        <join-column name="idCatArt" referenced-column-name="idCatArt"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idregion" target-entity="Region" fetch="LAZY">
      <join-columns>
        <join-column name="idRegion" referenced-column-name="idRegion"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="iduser" target-entity="Client" fetch="LAZY">
      <join-columns>
        <join-column name="idUser" referenced-column-name="idUser"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
